{
    "name": "afoozle/newrelic-tools",
    "description": "New Relic Tools",
    "keywords": [
        "package"
    ],
    "homepage": "https://github.com/afoozle/newrelic-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Wheeler",
            "email": "matt@yurisko.net",
            "homepage": "http://www.yurisko.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "0.9.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Afoozle\\NewrelicTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afoozle\\NewrelicTools\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
